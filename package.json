{
	"name": "connect-redis-session",
	"version": "1.0.6",
	"description": "Redis session store for Express",
	"license": "MIT",
	"author": {
		"name": "Rahil Patel",
		"url": "https://github.com/rahil-p"
	},
	"homepage": "https://github.com/rahil-p/connect-redis-session#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/rahil-p/connect-redis-session.git"
	},
	"bugs": {
		"url": "https://github.com/rahil-p/connect-redis-session/issues"
	},
	"files": [
		"build/lib/**/*.js",
		"build/lib/**/*.d.ts",
		"build/lua/**/*.lua"
	],
	"main": "build/lib/index.js",
	"scripts": {
		"build": "npm run-script build:lua && npm run-script build:ts",
		"build:lua": "node .build.js",
		"build:ts": "tsc --project .",
		"lint": "eslint --fix .",
		"test": "rm -rf build && npm run-script build && npm run-script test:report",
		"test:report": "nyc npm run-script test:test",
		"test:test": "ts-mocha test/**/*.spec.ts"
	},
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "^1.0.2",
		"@types/chai": "^4.3.0",
		"@types/express-session": "^1.17.0",
		"@types/mocha": "^9.1.0",
		"@types/sinon": "^10.0.8",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"chai": "^4.3.4",
		"codecov": "^3.8.2",
		"eslint": "^8.7.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-mocha": "^10.0.3",
		"eslint-plugin-prettier": "^4.0.0",
		"express-session": "^1.17.0",
		"luamin": "^1.0.4",
		"mocha": "^9.2.0",
		"nyc": "^15.1.0",
		"prettier": "^2.5.1",
		"redis": "^4.0.0",
		"sinon": "^12.0.1",
		"source-map-support": "^0.5.21",
		"testcontainers": "^8.2.0",
		"ts-mocha": "^9.0.2",
		"typescript": "^4.5.5"
	},
	"peerDependencies": {
		"express-session": "^1.17.0",
		"redis": "^4.0.0"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"keywords": [
		"redis",
		"express",
		"session",
		"store"
	],
	"nyc": {
		"extends": "@istanbuljs/nyc-config-typescript",
		"reporter": [
			"lcovonly",
			"html",
			"text",
			"text-summary"
		],
		"all": true,
		"cache": false,
		"instrument": true,
		"sourceMap": true,
		"check-coverage": true,
		"include": [
			"lib/**/*"
		]
	},
	"eslintConfig": {
		"env": {
			"es6": true,
			"node": true
		},
		"ignorePatterns": [
			"build/**/*"
		],
		"extends": [
			"eslint:recommended",
			"airbnb-base",
			"plugin:prettier/recommended",
			"plugin:eslint-comments/recommended",
			"plugin:mocha/recommended",
			"plugin:import/typescript",
			"plugin:@typescript-eslint/eslint-recommended",
			"plugin:@typescript-eslint/recommended",
			"plugin:@typescript-eslint/recommended-requiring-type-checking"
		],
		"parser": "@typescript-eslint/parser",
		"parserOptions": {
			"project": "./tsconfig.json"
		},
		"plugins": [
			"mocha",
			"@typescript-eslint"
		],
		"rules": {
			"@typescript-eslint/member-ordering": 1,
			"@typescript-eslint/no-empty-function": 0,
			"@typescript-eslint/no-use-before-define": 1,
			"@typescript-eslint/prefer-nullish-coalescing": 1,
			"@typescript-eslint/prefer-optional-chain": 1,
			"@typescript-eslint/quotes": [
				1,
				"single",
				{
					"avoidEscape": true
				}
			],
			"arrow-body-style": 0,
			"import/extensions": [
				1,
				"ignorePackages",
				{
					"js": "never",
					"ts": "never"
				}
			],
			"import/no-extraneous-dependencies": [
				1,
				{
					"devDependencies": [
						"**/*.spec.ts"
					]
				}
			],
			"import/prefer-default-export": 0,
			"lines-between-class-members": [
				1,
				"always",
				{
					"exceptAfterSingleLine": true
				}
			],
			"mocha/no-sibling-hooks": 0,
			"no-shadow": 0,
			"no-underscore-dangle": 0,
			"no-use-before-define": 0,
			"no-useless-constructor": 0,
			"prefer-arrow-callback": 0,
			"prettier/prettier": [
				1,
				{
					"arrowParens": "avoid",
					"printWidth": 120,
					"singleQuote": true,
					"trailingComma": "all"
				}
			]
		},
		"settings": {
			"import/resolver": {
				"node": {
					"paths": [
						"src",
						"test"
					],
					"extensions": [
						".ts"
					]
				}
			},
			"import/parsers": {
				"@typescript-eslint/parser": [
					".ts"
				]
			}
		}
	}
}
